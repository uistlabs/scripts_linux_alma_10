-D
-b 8192
-f 1
-a always,exit -F arch=b64 -S adjtimex,settimeofday,clock_settime -k time-change
-a always,exit -F arch=b32 -S adjtimex,settimeofday,clock_settime -k time-change
-a always,exit -F arch=b64 -F path=/etc/localtime -F perm=wa -k time-change
-a always,exit -F arch=b32 -F path=/etc/localtime -F perm=wa -k time-change
-a always,exit -F arch=b64 -S sethostname,setdomainname -k identity
-a always,exit -F arch=b32 -S sethostname,setdomainname -k identity
-a always,exit -F arch=b64 -F path=/etc/issue        -F perm=wa -k identity
-a always,exit -F arch=b32 -F path=/etc/issue        -F perm=wa -k identity
-a always,exit -F arch=b64 -F path=/etc/hosts        -F perm=wa -k identity
-a always,exit -F arch=b32 -F path=/etc/hosts        -F perm=wa -k identity
-a always,exit -F arch=b64 -F path=/etc/hostname     -F perm=wa -k identity
-a always,exit -F arch=b32 -F path=/etc/hostname     -F perm=wa -k identity
-a always,exit -F arch=b64 -F path=/etc/resolv.conf  -F perm=wa -k identity
-a always,exit -F arch=b32 -F path=/etc/resolv.conf  -F perm=wa -k identity
-a always,exit -F arch=b64 -S init_module,finit_module,delete_module -k modules
-a always,exit -F arch=b32 -S init_module,delete_module -k modules
-a always,exit -F arch=b64 -F path=/usr/lib/modules -F perm=wa -k modules
-a always,exit -F arch=b32 -F path=/usr/lib/modules -F perm=wa -k modules
-a always,exit -F arch=b64 -S mount,umount2,pivot_root -k mount
-a always,exit -F arch=b32 -S mount,umount2 -k mount
-a always,exit -F arch=b64 -S chmod,fchmod,fchmodat,chown,fchown,fchownat,lchown,utime,utimes,utimensat \
  -F auid>=1000 -F auid!=-1 -k perm-change
-a always,exit -F arch=b32 -S chmod,fchmod,fchmodat,chown,fchown,fchownat,lchown,utime,utimes,utimensat \
  -F auid>=1000 -F auid!=-1 -k perm-change
-a always,exit -F arch=b64 -S unlink,unlinkat,rename,renameat,renameat2 \
  -F auid>=1000 -F auid!=-1 -k delete
-a always,exit -F arch=b32 -S unlink,unlinkat,rename,renameat \
  -F auid>=1000 -F auid!=-1 -k delete
-a always,exit -F arch=b64 -F path=/etc/passwd          -F perm=wa -k auth-files
-a always,exit -F arch=b32 -F path=/etc/passwd          -F perm=wa -k auth-files
-a always,exit -F arch=b64 -F path=/etc/group           -F perm=wa -k auth-files
-a always,exit -F arch=b32 -F path=/etc/group           -F perm=wa -k auth-files
-a always,exit -F arch=b64 -F path=/etc/shadow          -F perm=wa -k auth-files
-a always,exit -F arch=b32 -F path=/etc/shadow          -F perm=wa -k auth-files
-a always,exit -F arch=b64 -F path=/etc/gshadow         -F perm=wa -k auth-files
-a always,exit -F arch=b32 -F path=/etc/gshadow         -F perm=wa -k auth-files
-a always,exit -F arch=b64 -F path=/etc/ssh/sshd_config -F perm=wa -k ssh
-a always,exit -F arch=b32 -F path=/etc/ssh/sshd_config -F perm=wa -k ssh
-a always,exit -F arch=b64 -F path=/etc/sudoers       -F perm=wa -k sudo
-a always,exit -F arch=b32 -F path=/etc/sudoers       -F perm=wa -k sudo
-a always,exit -F arch=b64 -F path=/etc/sudoers.d     -F perm=wa -k sudo
-a always,exit -F arch=b32 -F path=/etc/sudoers.d     -F perm=wa -k sudo
-a always,exit -F arch=b64 -F path=/usr/bin/sudo      -F perm=x  -F auid>=1000 -F auid!=-1 -k sudo
-a always,exit -F arch=b32 -F path=/usr/bin/sudo      -F perm=x  -F auid>=1000 -F auid!=-1 -k sudo
-a always,exit -F arch=b64 -F path=/bin/su            -F perm=x  -F auid>=1000 -F auid!=-1 -k su
-a always,exit -F arch=b32 -F path=/bin/su            -F perm=x  -F auid>=1000 -F auid!=-1 -k su
-a always,exit -F arch=b64 -F path=/etc/audit/        -F perm=wa -k auditcfg
-a always,exit -F arch=b32 -F path=/etc/audit/        -F perm=wa -k auditcfg
-a always,exit -F arch=b64 -F path=/usr/sbin/auditd   -F perm=x  -k auditd
-a always,exit -F arch=b32 -F path=/usr/sbin/auditd   -F perm=x  -k auditd
-e 2
